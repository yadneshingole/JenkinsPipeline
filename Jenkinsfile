pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
        
                stage('build') {
            steps {
                echo 'Building'
            }
        }
        
                stage('deploy') {
            steps {
                echo 'deploying'
            }
        }
        
                stage('test') {
            steps {
                echo 'testing in server'
            }
        }
        
    }
    
}
